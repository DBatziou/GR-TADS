modify Traveler
    /*
     *   Get my location's name, from the PC's perspective, for describing
     *   my arrival to or departure from my current location.  We'll
     *   simply return our location's destName, or "the area" if it
     *   doesn't have one.
     */
    travelerLocName()
    { 
        /* get our location's name from the PC's perspective */
		withCaseNominative;
        local nm = location.getDestName(gPlayerChar, gPlayerChar.location);
		//###We need to add the article of the location###
		replaceNounEndings(nm);
		replaceEndings(nm);
		if (!location.isProperName) {
            if (location.isHim)
                nm = 'ο '+ nm;
            if (location.isHer)
                nm = 'η '+ nm; 
			if (location.isIt)
                nm = 'το '+ nm;   			
            if (location.isHimPlural)
                nm = 'οι '+ nm;
			if (location.isHerPlural)
                nm = 'οι '+ nm;
			if (location.isItPlural)
                nm = 'τα '+ nm;
            if (!location.isHim && !location.isHer && !location.isHimPlural  && !location.isHerPlural  && !location.isItPlural)
                nm = 'το '+ nm;
        }
		

        /* if there's a name, return it; otherwise, use "the area" */
        return (nm != nil ? nm : 'η περιοχή');
    }

	tonTravelerLocName()
    {
        /* get our location's name from the PC's perspective */
        withCaseAccusative;
        local nm = location.getDestName(gPlayerChar, gPlayerChar.location);
		replaceNounEndings(nm);
		replaceEndings(nm);
		if (!location.isProperName) {
            if (location.isHim)
                nm = 'τον '+ nm;
            if (location.isHer)
                nm = 'την '+ nm;   			
            if (location.isHimPlural)
                nm = 'τους '+ nm;
			if (location.isHerPlural)
                nm = 'τις '+ nm;
			if (location.isItPlural)
                nm = 'τα '+ nm;
            if (!location.isHim && !location.isHer && !location.isHimPlural  && !location.isHerPlural  && !location.isItPlural)
                nm = 'το '+ nm;
        }

        /* if there's a name, return it; otherwise, use "the area" */
        return (nm != nil ? nm :  'την περιοχή');
    }
	
	touTravelerLocName()
    {
        /* get our location's name from the PC's perspective */
        withCaseGenitive;
        local nm = location.getDestName(gPlayerChar, gPlayerChar.location);
		replaceNounEndings(nm);
		replaceEndings(nm);
		if (!location.isProperName) {
            if (location.isHim)
                nm = 'του '+ nm;
            if (location.isHer)
                nm = 'της '+ nm;   			
            if (location.isHimPlural)
                nm = 'των '+ nm;
			if (location.isHerPlural)
                nm = 'των '+ nm;
			if (location.isItPlural)
                nm = 'των '+ nm;
            if (!location.isHim && !location.isHer && !location.isHimPlural  && !location.isHerPlural  && !location.isItPlural)
                nm = 'του '+ nm;
        }

        /* if there's a name, return it; otherwise, use "the area" */
        return (nm != nil ? nm :  'της περιοχής');
    }
    
    stonTravelerLocName()
    {
        /* get our location's name from the PC's perspective */
        withCaseDative;
        local nm = location.getDestName(gPlayerChar, gPlayerChar.location);
		replaceNounEndings(nm);
		replaceEndings(nm);
		if (!location.isProperName) {
            if (location.isHim)
                nm = 'στον '+ nm;
            if (location.isHer)
                nm = 'στην '+ nm;   			
            if (location.isHimPlural)
                nm = 'στους '+ nm;
			if (location.isHerPlural)
                nm = 'στις '+ nm;
			if (location.isItPlural)
                nm = 'στα '+ nm;
            if (!location.isHim && !location.isHer && !location.isHimPlural  && !location.isHerPlural  && !location.isItPlural)
                nm = 'στο '+ nm;
        }
        /* if there's a name, return it; otherwise, use "the area" */
        return (nm != nil ? nm :  'στην περιοχή');
    }
    /*
     *   Get my "remote" location name, from the PC's perspective.  This
     *   returns my location name, but only if my location is remote from
     *   the PC's perspective - that is, my location has to be outside of
     *   the PC's top-level room.  If we're within the PC's top-level
     *   room, we'll simply return an empty string.
     */
    travelerRemoteLocName()
    {
        /*
         *   if my location is outside of the PC's outermost room, we're
         *   remote, so return my location name; otherwise, we're local,
         *   so we don't need a remote name at all
         */
        if (isIn(gPlayerChar.getOutermostRoom()))
            return '';
        else
            return travelerLocName;
    }
	tonTravelerRemoteLocName()
    {
        /*
         *   if my location is outside of the PC's outermost room, we're
