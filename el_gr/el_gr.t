     *   person for the nominative case, objective case, possessive
     *   adjective, possessive noun, and objective reflexive
     */
    itOnom
    {
        return ((gameMain.useCapitalizedAdress && gPlayerChar == self) ? '\^' : '') + 
				['εγώ', 'εγώ', 'εγώ', 'εγώ','εγώ','εγώ',
               'εσύ', 'εσύ', 'εσύ', 'εσύ','εσύ','εσύ',
               'αυτός', 'αυτή', 'αυτό', 'αυτοί', 'αυτές', 'αυτά',
			   'εμείς','εμείς','εμείς','εμείς','εμείς','εμείς',
			   'εσείς','εσείς','εσείς','εσείς','εσείς','εσείς',
			   'αυτοί', 'αυτές', 'αυτά', 'αυτοί', 'αυτές', 'αυτά'][pronounSelector];
    }
	
	itGen
    {
        return ((gameMain.useCapitalizedAdress && gPlayerChar == self) ? '\^' : '') +
            ['εμένα', 'εμένα', 'εμένα', 'εμένα','εμένα','εμένα',
               'εσένα', 'εσένα', 'εσένα', 'εσένα','εσένα','εσένα',
               'αυτού', 'αυτής', 'αυτού', 'αυτών', 'αυτών', 'αυτών',
			   'εμάς','εμάς','εμάς','εμάς','εμάς','εμάς',
			   'εσάς','εσάς','εσάς','εσάς','εσάς','εσάς',
			   'αυτών','αυτών','αυτών','αυτών','αυτών','αυτών'][pronounSelector];
    }
