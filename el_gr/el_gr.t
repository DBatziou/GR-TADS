    // Function to check if a word is in the list
    isWordInList(phrase)
    {
		// Split the phrase into individual words (based on spaces)
        local words = phrase.split(' ');
        
        // Extract the last word (the noun, presumably)
        local noun = words[words.length()];
       
        // Normalize the noun
        local normalizedNoun = noun.toLower();
		
		
        
        // Check if the noun is in the list
		foreach (local category in wordLists)
        {
            local categoryNumber = category[1]; // First element is the category number
            local list = category[2];          // Second element is the word list
            
            foreach (local listWord in list)
            {
                if (listWord.toLower() == normalizedNoun)
                {
                    // Return the category number if the word is found
					
                    return categoryNumber;
                }
            }
        }
		
		
        
		
        return nil; // Word not found
    
