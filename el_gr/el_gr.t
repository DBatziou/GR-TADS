	isProparoxytone(word) {
			// Greek vowels, accented vowels, and diphthongs
		local vowels = ['α', 'ε', 'η', 'ι', 'ο', 'υ', 'ω'];
		local accentedVowels = ['ά', 'έ', 'ή', 'ί', 'ό', 'ύ', 'ώ'];
		local diphthongs = ['αι', 'οι', 'ει', 'υι', 'ου', 'αυ', 'ευ'];

		local vowelCount = 0;
		local index = word.length();

		while (index > 0) {
			// Check for diphthongs first, just like before
			local foundDiphthong = nil;

			foreach (local d in diphthongs) {
				if (index >= d.length() && word.substr(index - d.length() + 1, d.length()) == d) {
					foundDiphthong = d;
					break;
				}
			}

			if (foundDiphthong) {
				// Treat diphthong as a single vowel
				vowelCount++;
				index -= foundDiphthong.length();
			} else {
				// Check for a single accented vowel
				local currentChar = word.substr(index, 1);

				if (accentedVowels.indexOf(currentChar) != nil) {
					// If it's accented, return the current vowel count + 1 (1-based position)
					return vowelCount + 1;
				} else if (vowels.indexOf(currentChar) != nil) {
					// Count it as a vowel but keep going if it's not accented
					vowelCount++;
				}
				index--;
			}
		}

		// If we go through the word and find no accented vowels, return 0
		return 0;
	}
	


