	modifyWord(input, pattern, sub)
{
    local parts = pattern.split('/');
    local accentPosition = toInteger(parts[1]); // Desired syllable position (1, 2, or 3)
    local newEnding = parts[2]; // Desired new ending

    // Split the input into words
    local words = input.split(' ');

    // Process the last word
    local word = removeAccents(words[words.length()]);
    //local vowelIndexes = findVowels(word);
	//"...<<vowelIndexes[1]>>...";

    // Adjust the ending
    word = word.substr(1, word.length() - sub) + newEnding;

    // Reapply the accent
    word = applyAccent(word, accentPosition);

    // Replace the last word and return the result
    words[words.length()] = word;
    return words.join(' ');
}

/* Remove accents from a word */
removeAccents(word)
{
    local accented = ['ά', 'έ', 'ή', 'ί', 'ό', 'ύ', 'ώ', 'ϊ', 'ϋ', 'ΐ', 'ΰ'];
    local unaccented = ['α', 'ε', 'η', 'ι', 'ο', 'υ', 'ω', 'ι', 'υ', 'ι', 'υ'];

    foreach (local i in accented)
        word = word.findReplace(i, unaccented[accented.indexOf(i)], ReplaceAll);

    return word;
}

/* Find the indexes of vowels in the word, considering diphthongs */
findVowels(word)
{
    local vowels = ['α', 'ε', 'η', 'ι', 'ο', 'υ', 'ω'];
    local diphthongs = ['αι', 'ει', 'οι', 'υι', 'ου', 'ευ', 'αυ'];
    local indexes= new Vector(8);
    local len = word.length();
    local i = 1;
	//"Testing substr: word = <<word>>, i = 1, char = <<word.substr(1, 1)>>\n";


    while (i <= len)
    {
        if (i < len)
        {
            local pair = word.substr(i, 2);
			
            if (diphthongs.indexOf(pair) != nil)
            {
                indexes.append(i);
				
                i += 2; // Skip the diphthong
				
                continue;
            }
        }
		
		local char = word.substr(i, 1);
        if (vowels.indexOf(word.substr(i, 1)) != nil)
            indexes.append(i);
		
        i++;
		
    }
	
	
    return indexes;
}

countVowels(word)
{
    local vowels = ['α', 'ε', 'η', 'ι', 'ο', 'υ', 'ω'];
    local diphthongs = ['αι', 'ει', 'οι', 'υι', 'ου', 'ευ', 'αυ'];
    local vCounter=0;
    local len = word.length();
    local i = 1;
	//"Testing substr: word = <<word>>, i = 1, char = <<word.substr(1, 1)>>\n";


    while (i <= len)
    {
        if (i < len)
        {
            local pair = word.substr(i, 2);
			
            if (diphthongs.indexOf(pair) != nil)
            {
                vCounter+=1;
				
                i += 2; // Skip the diphthong
				
				
                continue;
            }
        }
		
		local char = word.substr(i, 1);
        if (vowels.indexOf(word.substr(i, 1)) != nil)
		vCounter+=1;
		

        i++;
		
		
    }
	
	
    return vCounter;
}


/* Apply the accent to the specified syllable */
applyAccent(word, n)
{
	local vCounter = countVowels(word);
    local vowels = ['α', 'ε', 'η', 'ι', 'ο', 'υ', 'ω'];
    local accentedVowels = ['ά', 'έ', 'ή', 'ί', 'ό', 'ύ', 'ώ'];
    local diphthongs = ['αι', 'ει', 'οι', 'υι', 'ου', 'ευ', 'αυ'];
    local vowelIndexes = findVowels(word);

    // If there are not enough vowels to apply the accent, return the word as is
    if (vowelIndexes.length() < n)
        return word;

    // Find the index of the vowel to accent (1-based indexing)
	///"....<<vowelIndexes.length()>>....";
    local targetIndex = vowelIndexes[vCounter - n + 1];
	 
    // Handle diphthongs and single vowels
    foreach (local d in diphthongs)
    {
        if (word.substr(targetIndex, d.length()) == d)
        {
            // Accent the correct character in the diphthong
            local accentedDiphthong =
                d.substr(1, 1) + accentedVowels[vowels.indexOf(d.substr(2, 1))];
            return word.substr(1, targetIndex - 1) +
                   accentedDiphthong +
                   word.substr(targetIndex + d.length());
        }
    }

    // For single vowels, replace with the accented version
    local vowelChar = word.substr(targetIndex, 1);
    if (vowels.indexOf(vowelChar) != nil)
    {
        return word.substr(1, targetIndex - 1) +
               accentedVowels[vowels.indexOf(vowelChar)] +
               word.substr(targetIndex + 1);
    }
	
	

    return word;
}


